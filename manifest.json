{
    "name": "NYU RMP & Course Evaluations",
    "description": "Only take easy classes!",
    "version": "1.0",
    "manifest_version": 3,
    "background": {
        "service_worker": "background.js"
    },
    "content_scripts": [{
        "js": ["main-script.js"]
    }],
    "permissions": ["scripting",
            "activeTab",
            "https://sis.portal.nyu.edu/*",
            "https://search-production.ratemyprofessors.com/*"
    ],
    "action": {
        "default_popup": "popup.html"
    }
}